{
    "@context": {
        "dact": "http://www.w3.org/ns/dcat#",
        "ids": "https://w3id.org/idsa/core/",
        "idsc": "https://w3id.org/idsa/code/",
        "odrl": "http://www.w3.org/ns/odrl/2/",
        "xsd:": "http://www.w3.org/2001/XMLSchema#",
        "foaf": "http://xmlns.com/foaf/0.1/",
        "dct": "http://purl.org/dc/terms/"
    },
    "@id": "http://provider.com/catalog1/dataset1/",
    "@type": "dcat:Dataset",
    "dct:title" : "IDS Dataset Collection",
    "dct:description": [ {
        "@language": "en",
        "@value": "This dataset includes all assets which identifiers comply to the pattern 'urn:guid:[A-F0-9]{8}\-[A-F0-9]{4}\-[A-F0-9]{4}\-[A-F0-9]{4}\-[A-F0-9]{12}'. A download via a S3 dataplane costs 5 EUR, while the HTTP_REST dataplane option costs only 2 EUR. The consumer must be a TRUST_PLUS certfified connector. No further distribution is allowed."
    }, {
        "@language": "de",
        "@value": "Dieses Dataset bezeichnet alle Assets, deren Identifier dem Pattern 'urn:guid:[A-F0-9]{8}\-[A-F0-9]{4}\-[A-F0-9]{4}\-[A-F0-9]{4}\-[A-F0-9]{12}' folgen. Ein Abruf über eine S3 Dataplane kostet 5 EUR und über eine HTTP_REST Dataplane 2 EUR. Der Consumer muss ein TRUST_PLUS zertifizierter Connetor sein. Eine Weiterverbreitung ist nicht gestattet."
    } ],
    "dcat:keyword": [
        "IDS",
        "data asset",
        "dcat dataset",
        "collection"
    ],
    "dcat:distribution": [ {
        "@type": "dcat:Distribution",
        "dct:title" : "Distribution#1",
        "dct:description": "This is a distribution everyone can see.",
        "dcat:keyword": [
            "dataplane",
            "public",
            "distribution"
        ],
        "ids:dataPlaneKind": {
            "@id": "idsc:HTTP_REST"
        },
        "dcat:hasPolicy": {
            "@type": "odrl:Offer",
            "@id": "https://provider.com/edc/offer/1",
            "odrl:permission": [ {
                "odrl:action": {"@id": "odrl:use"},
                "odrl:constraint" : [ {
                    "odrl:leftOperand": "idsc:SECURITY_PROFILE",
                    "odrl:operand": "odrl:equals",
                    "odrl:rightOperand": "idsc:TRUST_PLUS"
                } ],
                "odrl:duty": [ {
                    "odrl:action": {"@id": "odrl:pay"},
                    "odrl:constraint": [ {
                        "odrl:leftOperand": "idsc:DATA_PLANE",
                        "odrl:operand": "odrl:equals",
                        "odrl:rightOperand": "idsc:HTTP_REST"
                    }, {
                        "odrl:leftOperand": "odrl:payAmount",
                        "odrl:operand": "odrl:equals",
                        "odrl:rightOperand": {
                            "@type": "xsd:decimal",
                            "@value": "2.0"
                        },
                        "odrl:unit": {
                            "@id": "http://dbpedia.org/resource/Euro"
                        }
                    } ] 
                } ]
            } ],
            "odrl:prohibition": [ {
                "odrl:action": {"@id": "odrl:distribute"}
            } ]
        },
        "dcat:accessService": [ {
            "@type": [ "dcat:DataService", "idsc:NEGOTIATION_SERVICE" ],
            "dcat:endpointURL": {
                "@type": "xsd:anyURI",
                "@value": "https://provider.com/connector/negotiation"
            }
        }, {
            "@type": [ "dcat:DataService", "idsc:TRANSFER_SERVICE" ],
            "dcat:endpointURL": {
                "@type": "xsd:anyURI",
                "@value": "https://provider.com/connector/transfer"
            }
        } ]
    }, {
        "@type": "dcat:Distribution",
        "dct:title" : "Distribution#2",
        "dct:description": "This is another distribution.",
        "dcat:keyword": [
            "dataplane",
            "public",
            "distribution"
        ],
        "ids:dataPlaneKind": {
            "@id": "idsc:S3"
        },
        "dcat:hasPolicy": {
            "@type": "odrl:Offer",
            "@id": "https://provider.com/edc/offer/1",
            "odrl:permission": [ {
                "odrl:action": {"@id": "odrl:use"},
                "odrl:constraint" : [ {
                    "odrl:leftOperand": "idsc:SECURITY_PROFILE",
                    "odrl:operand": "odrl:equals",
                    "odrl:rightOperand": "idsc:TRUST_PLUS"
                } ],
                "odrl:duty": [ {
                    "odrl:action": {"@id": "odrl:pay"},
                    "odrl:constraint": [ {
                        "odrl:leftOperand": "idsc:DATA_PLANE",
                        "odrl:operand": "odrl:equals",
                        "odrl:rightOperand": "idsc:S3"
                    }, {
                        "odrl:leftOperand": "odrl:payAmount",
                        "odrl:operand": "odrl:equals",
                        "odrl:rightOperand": {
                            "@type": "xsd:decimal",
                            "@value": "5.0"
                        },
                        "odrl:unit": {
                            "@id": "http://dbpedia.org/resource/Euro"
                        }
                    } ]
                } ]
            } ],
            "odrl:prohibition": [ {
                "odrl:action": {"@id": "odrl:distribute"}
            } ]
        },
        "dcat:accessService": [ {
            "@type": [ "dcat:DataService", "idsc:NEGOTIATION_SERVICE" ],
            "dcat:endpointURL": {
                "@type": "xsd:anyURI",
                "@value": "https://provider.com/connector/negotiation"
            }
        }, {
            "@type": [ "dcat:DataService", "idsc:TRANSFER_SERVICE" ],
            "dcat:endpointURL": {
                "@type": "xsd:anyURI",
                "@value": "https://provider.com/connector/transfer"
            }
        } ]
    } ]
}